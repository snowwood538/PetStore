<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Wiremock Test">
    <test name="Get requests testing">
        <groups>
            <run>
                <include name="Wiremock Test Group"/>
            </run>
        </groups>
        <classes>
            <class name="swagger.petstore.hooks.SuiteHooks"/>
            <class name="swagger.petstore.pet.wiremock.get.by.id.GetByIdPositiveTest"/>
            <class name="swagger.petstore.pet.wiremock.get.by.id.GetPetByNotExistingIdNegativeTest"/>
            <class name="swagger.petstore.pet.wiremock.get.by.id.GetByNotValidIDNegativeTest"/>
            <class name="swagger.petstore.pet.wiremock.post.pet.PostNewPetWithAllFieldsPositiveTest"/>
            <class name="swagger.petstore.pet.wiremock.post.pet.PostNewPetWithRequiredFieldsPositiveTest"/>
        </classes>
    </test>
</suite>